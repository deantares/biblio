@Article{parallel90,
  author = 	 { I. Garc\'ia; Juan-Juli\'an Merelo-Guerv\'os;  J. D. Bruguera; E. L. Zapata},
  title = 	 { Parallel Quadrant Interlocking Factorization on Hypercube Computer},
  journal = 	 {J. Parallel Computing},
  year = 	 1990,
  volume =	 15,
number = {1--3},
  pages =	 {87-100},
  abstract = {In this article we present an adaptation of the QIF (Quadrant Interlocking Factorization) algorithm, which solves systems of linear equations, for implementation in SIMD hypercube computers with distributed memory. This method is based in the WZ decomposition of the system's matrix. The parallel algorithm developed is general in the sense that there is no restriction imposed on the size of the problem and that it is independent of the dimension of the hypercube. The comparison of this algorithm with the parallel algorithms based on the LU factorization show that the execution time is divided by a factor of two, approximately},
   note = {\htmladdnormallink{doi:10.1016/0167-8191(90)90033-6}{http://dx.doi.org/10.1016/0167-8191(90)90033-6}}
  annote = {Impact Factor: 0.625, references (WOS): 6 (3 self-citations)}
}

