	month = {},
	pages = {61--71},
	title = {Use of innovative technologies on an e-learning course},
	url = {http://www.sciencedirect.com/science/article/B6W4X-4FPDRGW-1/2/619b7c8195cec550aa31bb3b14489de2},
	volume = {8},
